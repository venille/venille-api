{
  "name": "venille-server",
  "version": "1.0.0",
  "description": "Venille server(v1)",
  "author": "Venille.",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && npx typeorm -d dist/libs/common/src/database/database-source.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@google/genai": "^0.12.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.2",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.3",
    "@slack/webhook": "^7.0.3",
    "@types/lodash": "^4.17.7",
    "@types/stripe-v3": "^3.1.33",
    "apple-signin-auth": "^1.7.8",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bootstrap-email": "^1.2.9",
    "cache-manager": "^5.7.1",
    "cache-manager-memcached-store": "^6.0.1",
    "cache-manager-redis-store": "^3.0.1",
    "cd": "^0.3.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-color": "^2.0.4",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "etag": "^1.8.1",
    "firebase-admin": "^12.5.0",
    "format-currency": "^1.0.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "helmet": "^7.1.0",
    "jest-mock-extended": "^3.0.7",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mongoose": "^8.5.0",
    "mongoose-field-encryption": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nestjs-pino": "^4.1.0",
    "papaparse": "^5.5.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "paystack-sdk": "^2.5.16",
    "pg": "^8.11.3",
    "pino-http": "^10.2.0",
    "puppeteer": "^23.8.0",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.4",
    "streamifier": "^0.1.1",
    "string-comparison": "^1.3.0",
    "stripe": "^17.5.0",
    "typeorm": "^0.3.17",
    "ua-parser-js": "^2.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.10",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.17.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/auth-service(|/.*)$": "<rootDir>/libs/auth-service/src/$1",
      "^@app/account-service(|/.*)$": "<rootDir>/libs/account-service/src/$1"
    }
  }
}
